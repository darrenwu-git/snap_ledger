{
  "name": "snap_ledger",
  "private": true,
  "version": "0.2.6",
  "type": "module",
  "scripts": {
    "dev": "vite --port 4173",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node server.js",
    "test": "vitest",
    "release:patch": "npm version patch -m \"chore(release): %s\" && git push origin dev --follow-tags",
